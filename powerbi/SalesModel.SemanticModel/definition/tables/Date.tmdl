	partition Date = calculated
		mode: import
		source = ```
				
				ADDCOLUMNS(
				    CALENDARAUTO(),
				    "Year", YEAR([Date]),
				    "Quarter", "Q" & FORMAT([Date], "Q"),
				    "MonthNo", MONTH([Date]),
				    "MonthName", FORMAT([Date], "MMMM"),
				    "YearMonth", FORMAT([Date], "yyyy-MM"),
				    "WeekdayNo", WEEKDAY([Date], 2),   // 2 = Monday=1, Sunday=7; change if you prefer different start
				    "WeekdayName", FORMAT([Date], "dddd")
				)
				
